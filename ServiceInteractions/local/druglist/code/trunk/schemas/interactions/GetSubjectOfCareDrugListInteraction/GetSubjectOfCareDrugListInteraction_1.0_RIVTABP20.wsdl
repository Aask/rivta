<wsdl:definitions name='GetSubjectOfCareDrugListInteraction'
	xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/' xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns:wsa='http://www.w3.org/2005/08/addressing'
	xmlns:tjsr='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugListResponder:1'
	xmlns:tjsi='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugListInitiator:1'
	xmlns:tns='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugList:1:rivtabp20'
	targetNamespace='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugList:1:rivtabp20'>
	<xs:annotation>
		<xs:documentation>
			Tjänsteinteraktionens namn: GetSubjectOfCareDrugListInteraction
			Beskrivning:
			Interaction to retrieve the currently active drug list of a subject of care at a
			care unit.
			Revisioner:
			Tjänstedomän: ehrexchange:medicament
			Tjänsteinteraktionstyp: Fråga-Svar
			RIV Teknisk Anvisning: RIVTABP20
			Förvaltas av: Luleå Tekiska Universitet/Norrbottens Läns Landsting
		</xs:documentation>
	</xs:annotation>
	<wsdl:types>
		<xs:schema
			targetNamespace='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugListResponder:1'>
			<xs:import schemaLocation='GetSubjectOfCareDrugListResponder_1.0.xsd'
				namespace='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugListResponder:1' />
		</xs:schema>
		<xs:schema targetNamespace='http://www.w3.org/2005/08/addressing'>
			<xs:import schemaLocation='../../core_components/ws-addressing-1.0.xsd'
				namespace='http://www.w3.org/2005/08/addressing' />
		</xs:schema>
	</wsdl:types>
	<wsdl:message name='GetSubjectOfCareDrugListRequest'>
		<wsdl:part name='LogicalAddress' element='wsa:To'>
			<xs:annotation>
				<xs:documentation>LogicalAddress is the hsaid of the
					healthcare_facility that is the owner of the drug list of the
					subject of care.</xs:documentation>
			</xs:annotation>
		</wsdl:part>
		<wsdl:part name='parameters' element='tjsr:GetSubjectOfCareDrugList' />
	</wsdl:message>
	<wsdl:message name='GetSubjectOfCareDrugListResponse'>
		<wsdl:part name='parameters' element='tjsr:GetSubjectOfCareDrugListResponse' />
	</wsdl:message>
	<wsdl:portType name='GetSubjectOfCareDrugListResponderInterface'>
		<wsdl:operation name='GetSubjectOfCareDrugList'>
			<wsdl:input message='tns:GetSubjectOfCareDrugListRequest' />
			<wsdl:output message='tns:GetSubjectOfCareDrugListResponse' />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name='GetSubjectOfCareDrugListResponderBinding'
		type='tns:GetSubjectOfCareDrugListResponderInterface'>
		<soap:binding style='document'
			transport='http://schemas.xmlsoap.org/soap/http' />
		<wsdl:operation name='GetSubjectOfCareDrugList'>
			<soap:operation
				soapAction='urn:nll:ehrexchange:medicament:GetSubjectOfCareDrugListResponder:1:GetSubjectOfCareDrugList'
				style='document' />
			<wsdl:input>
				<soap:header use='literal' message='tns:GetSubjectOfCareDrugListRequest'
					part='LogicalAddress' />
				<soap:body use='literal' parts='parameters' />
			</wsdl:input>
			<wsdl:output>
				<soap:body use='literal' />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name='GetSubjectOfCareDrugListResponderService'>
		<wsdl:port name='GetSubjectOfCareDrugListResponderPort'
			binding='tns:GetSubjectOfCareDrugListResponderBinding'>
			<soap:address location='http://tempuri.org' />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>