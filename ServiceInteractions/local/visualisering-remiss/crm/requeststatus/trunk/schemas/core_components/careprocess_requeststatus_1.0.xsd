<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
	xmlns:tns='urn:riv:careprocess:requeststatus:1' targetNamespace='urn:riv:careprocess:requeststatus:1'
	xmlns:itr='urn:riv:itintegration:registry:1' xmlns:iso='urn:iso:21090:2008'
	elementFormDefault='qualified' attributeFormDefault='unqualified'
	version='1.0'>
	<xs:annotation>
		<xs:documentation>
			Beskrivning: Domänbegrepp som används av
			remisstatustjänsten.
			Revisioner: 2011-11-30: Version 1.0 - Skapade
			första version av domändefinitionen.
		</xs:documentation>
	</xs:annotation>

	<xs:import schemaLocation='itintegration_registry_1.0.xsd'
		namespace='urn:riv:itintegration:registry:1' />
	<xs:include schemaLocation="ISO_dt.xsd" />

	<xs:complexType name='StatusEventType'>
		<xs:sequence>
			<xs:element name='person-id' type='tns:II' />
			<xs:element name='sender-request-id' type='tns:II'
				minOccurs="0" />
			<xs:element name='receiver-request-id' type='tns:II'
				minOccurs="0" />
			<xs:element name='type-of-request' type='tns:CD' />
			<xs:element name='form-of-request' type='tns:CD'
				minOccurs='0' />
			<xs:element name='request-issued-by-person-id' type='tns:II'
				minOccurs='0' />
			<xs:element name='request-issued-by-person-name' type='tns:ST'
				minOccurs='0' />
			<xs:element name='request-issued-by-care-unit-id' type='tns:II'
				minOccurs='0' />
			<xs:element name='request-issued-by-care-unit-local-id'
				type='tns:II' minOccurs='0' />
			<xs:element name='request-issued-by-care-unit-description'
				type='tns:ST' minOccurs='0' />
			<xs:element name='receiving-person-name' type='tns:ST'
				minOccurs='0' />
			<xs:element name='receiving-care-unit-id' type='tns:II'
				minOccurs='0' />
			<xs:element name='receiving-care-unit-local-id' type='tns:II'
				minOccurs='0' />
			<xs:element name='receiving-care-unit-description' type='tns:ST'
				minOccurs='0' />
			<xs:element name='logical-system-id' type='tns:II' />
			<xs:element name='status-code' type='tns:CD' />
			<xs:element name='event-time' type="tns:TS" />
			<xs:element name='allowed-to-show' type="tns:BL" />

			<xs:any namespace=' #
				other ' processContents='lax'
				minOccurs='0' maxOccurs='unbounded' />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name='PersonIdType'>
		<xs:sequence>
			<xs:element name='Patient-id' type='xs:string' />
			<xs:any namespace='##other' processContents='lax' minOccurs='0'
				maxOccurs='unbounded' />
		</xs:sequence>
	</xs:complexType>
</xs:schema>