<wsdl:definitions 
	name="BirthRegistrationInteraction"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:rgy="urn:riv:itintegration:registry:1"
	xmlns:tjsr="urn:riv:healthcertificate:lifeline:BirthRegistrationResponder:1" 
	xmlns:tns="urn:riv:healthcertificate:lifeline:BirthRegistration:1:rivtabp21"
	targetNamespace="urn:riv:healthcertificate:lifeline:BirthRegistration:1:rivtabp21">

	<wsdl:documentation>
		Tjänsteinteraktionens namn: BirthRegistrationInteraction
		Beskrivning: 
			Interaction to register a new birth at Skatteverket and retrieve
			a new subject of care identifier (personnummer).
		Revisioner: 
			2011-11-10 Version 1.0, Magnus Ekstrand - Callista Enterprise AB
		Tjänstedomän: healthcertificate:lifeline
		Tjänsteinteraktionstyp: Fråga-svar
		RIV Teknisk Anvisning: Basic Profile 2.1
		Förvaltas av: VGR IT
	</wsdl:documentation>

	<wsdl:types>
		<xs:schema targetNamespace="urn:riv:healthcertificate:lifeline:BirthRegistration:1:rivtabp21">
			<xs:import schemaLocation="BirthRegistrationResponder_1.0.xsd" namespace="urn:riv:healthcertificate:lifeline:BirthRegistrationResponder:1"/>
			<xs:import schemaLocation="../../core_components/itintegration_registry_1.0.xsd" namespace="urn:riv:itintegration:registry:1"/>
		</xs:schema>
	</wsdl:types>

	<wsdl:message name="BirthRegistrationRequest">
		<xs:annotation>
			<xs:documentation>
				LogicalAddress is the organization id for Skatteverket.
			</xs:documentation>
		</xs:annotation>
        <wsdl:part name="LogicalAddress" element="rgy:LogicalAddress" />
		<wsdl:part name="parameters" element="tjsr:BirthRegistration" />
	</wsdl:message>

	<wsdl:message name="BirthRegistrationResponse">
		<wsdl:part name="parameters" element="tjsr:BirthRegistrationResponse"/>
	</wsdl:message>

	<wsdl:portType name="BirthRegistrationResponderInterface">
	
		<wsdl:operation name="BirthRegistration">
			<wsdl:input message="tns:BirthRegistrationRequest"/>
			<wsdl:output message="tns:BirthRegistrationResponse"/>
		</wsdl:operation>
		
	</wsdl:portType>
	
	<wsdl:binding name="BirthRegistrationResponderBinding" type="tns:BirthRegistrationResponderInterface">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>

		<wsdl:operation name="BirthRegistration">
			<soap:operation soapAction="urn:riv:healthcertificate:lifeline:BirthRegistrationResponder:1"/>
			<wsdl:input>
				<soap:header use="literal" message="tns:BirthRegistrationRequest" part="LogicalAddress"/>
				<soap:body use="literal" parts="parameters"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
		
	</wsdl:binding>

	<wsdl:service name="BirthRegistrationResponderService">
		<wsdl:port name="BirthRegistrationResponderPort" binding="tns:BirthRegistrationResponderBinding">
			<soap:address location="http://tempuri.org"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
