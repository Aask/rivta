<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="GetAvailableServiceProvidersInteraction" 
	xmlns:wsa="http://www.w3.org/2005/08/addressing" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tjs="urn:riv:crm:carelisting:GetAvailableServiceProvidersResponder:1" 
	targetNamespace="urn:riv:crm:carelisting:GetAvailableServiceProviders:1:rivtabp20" xmlns="urn:riv:crm:carelisting:GetAvailableServiceProviders:1:rivtabp20">

	<xsd:annotation>
		<xsd:documentation>
			Beskrivning: Tjänstekontraktet som ingår NationellaListingsstjänten.
			Revisioner: 2009-06-15 Version 1.0: Uppdelat i flera meddelanden (en per operation) efter 
							kommentarer ifrån Arkitekturledningen.
							2009-06-22 version 1.0.1: Plockade bort wsa:Action ifrån wsdl:portType. 
			Tjänstedomän: crm:carelisting. crm=Customer Relationship Management.
			Tjänsteinteraktionens namn: GetAvailableServiceProviders.
			Tjänsteinteraktionstyp: Fråga-Svar.	
			RIV Tekniska Anvisning: Basic Profile 2.0
		</xsd:documentation>
	</xsd:annotation>

	<!-- Egendefinierade typer -->
	<wsdl:types>
		<xsd:schema elementFormDefault="qualified">
			<xsd:import schemaLocation="GetAvailableServiceProviders_1_0.xsd" 
				namespace="urn:riv:crm:carelisting:GetAvailableServiceProvidersResponder:1" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import schemaLocation="ws-addressing-1.0.xsd"
				namespace="http://www.w3.org/2005/08/addressing" />
		</xsd:schema>
	</wsdl:types>

	<!-- Meddelanden -->
	<!-- Hämtar tillgängliga tjänsteutövare (vårdenheter). -->
	<wsdl:message name="getAvailableServiceProvidersRequest">
		<wsdl:part name="LogicalAddress" element="wsa:To" />
		<wsdl:part name="parameters" element="tjs:getAvailableServiceProviders" />
	</wsdl:message>
	<wsdl:message name="getAvailableServiceProvidersResponse">
		<wsdl:part name="parameters" element="tjs:getAvailableServiceProvidersResponse" />
	</wsdl:message>
	<wsdl:message name="TechnicalException">
		<wsdl:part name="parameters" element="tjs:TechnicalException" />
	</wsdl:message>
	
	<!-- Port typer -->
	<wsdl:portType name="GetAvailableServiceProvidersResponderInterface">
		<wsdl:operation name="getAvailableServiceProviders">
			<wsdl:documentation>
				Hämtar en lista med möjliga tjänsteutövare som medborgaren kan välja att
				lista sig på. Logisk adress är till vårdgivare som ska kunna svara på frågan.
			</wsdl:documentation>
			<wsdl:input message="getAvailableServiceProvidersRequest"/>
			<wsdl:output message="getAvailableServiceProvidersResponse" />
			<wsdl:fault name="TechnicalException" message="TechnicalException"></wsdl:fault>		
		</wsdl:operation>

	</wsdl:portType>

	<!-- Bindningar -->
	<wsdl:binding name="GetAvailableServiceProvidersResponderBinding" type="GetAvailableServiceProvidersResponderInterface">
		<soap:binding transport="http://schemas.xmlsoap.org/soap/http"
			style="document" />
		<wsdl:operation name="getAvailableServiceProviders">
			<soap:operation soapAction="urn:riv:crm:carelisting:GetAvailableServiceProvidersResponder:1:getAvailableServiceProviders" />
			<wsdl:input>
				<soap:header use="literal" message="getAvailableServiceProvidersRequest"
					part="LogicalAddress" />
				<soap:body use="literal" parts="parameters" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
			<wsdl:fault name="TechnicalException">
				<soap:fault use="literal" name="TechnicalException" />
			</wsdl:fault>			
		</wsdl:operation>
	</wsdl:binding>

	<!-- Servicar -->
	<wsdl:service name="GetAvailableServiceProvidersResponderService">
		<wsdl:port name="GetAvailableServiceProvidersResponderPort" binding="GetAvailableServiceProvidersResponderBinding">
			<soap:address location="http://temp.uri/" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
