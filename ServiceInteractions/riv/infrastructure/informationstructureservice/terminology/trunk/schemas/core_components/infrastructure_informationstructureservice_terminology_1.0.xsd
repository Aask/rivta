<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:riv:infrastructure:informationstructureservice:terminology:1" targetNamespace="urn:riv:infrastructure:informationstructureservice:terminology:1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<!-- Simple types -->
