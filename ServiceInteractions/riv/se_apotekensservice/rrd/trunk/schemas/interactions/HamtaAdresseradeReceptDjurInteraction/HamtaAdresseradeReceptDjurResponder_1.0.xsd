<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:rrd="urn:riv:se.apotekensservice:rrd:1" xmlns="urn:riv:se.apotekensservice:rrd:HamtaAdresseradeReceptDjurResponder:1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:riv:se.apotekensservice:rrd:HamtaAdresseradeReceptDjurResponder:1" attributeFormDefault="unqualified" elementFormDefault="qualified" version="1.0">
  <xs:import schemaLocation="../../core_components/se.apotekensservice_rrd_1.0.xsd" namespace="urn:riv:se.apotekensservice:rrd:1"/>
  <xs:element name="HamtaAdresseradeReceptDjur" type="HamtaAdresseradeReceptDjurRequestType"/>
  <xs:element name="HamtaAdresseradeReceptDjurResponse" type="HamtaAdresseradeReceptDjurResponseType"/>
  <xs:element name="ApplicationException" type="rrd:ApplicationException"/>
  <xs:element name="SystemException" type="rrd:SystemException"/>
  <xs:complexType name="HamtaAdresseradeReceptDjurRequestType">
    <xs:sequence>
      <xs:element nillable="false" name="franDatum" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation>Periodstart för vilken sökningen avser. Kan vara dagens datum eller
tidigare.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element nillable="false" name="klientinformation" type="rrd:KlientinformationRequest">
        <xs:annotation>
          <xs:documentation>Information om klienten som anropar tjänsten.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="tillDatum" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Periodslut för vilken sökningen avser. Kan vara dagens datum eller
tidigare.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:any processContents="lax" maxOccurs="unbounded" namespace="##other" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="HamtaAdresseradeReceptDjurResponseType">
    <xs:sequence>
      <xs:element nillable="false" name="receptInformationLista" maxOccurs="unbounded" type="rrd:ReceptInformationResponse">
        <xs:annotation>
          <xs:documentation>Lista med receptinformation. Varje objekt innehåller en receptsamling
samt eventuell information om förskriven vara, arbetsplats och
förskrivare. Tillhör flera efterfrågade receptval samma receptsamling så
returneras de i ett objekt i listan.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:any processContents="lax" maxOccurs="unbounded" namespace="##other" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
