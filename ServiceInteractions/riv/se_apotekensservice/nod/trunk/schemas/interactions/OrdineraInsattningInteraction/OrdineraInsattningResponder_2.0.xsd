		</xs:sequence>
        </xs:complexType>

<xs:complexType name="ForskrivningsdoseringRequest">
            <xs:sequence>                
                <xs:element name="doseringsschema" type="DoseringsschemaRequest" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Eventuellt doseringsschema</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="doseringstext" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Doseringstext. Den totala längden av doseringstext och ordinerad användning på det ordinerade läkemedlet får ej överstiga 256 tecken.</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="OrdineratLakemedelRequest">
            <xs:sequence>                
                <xs:element name="lakemedelsbeskrivning" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Beskrivning av ordinerat läkemedel.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="nplId" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Artikelns NPL-ID</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="nplPackId" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Artikelns NPLPack-ID</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="ordineradAnvandning" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Användning. Den totala längden av doseringstext på den ordinerade doseringen och den ordinerade användningen får ej överstiga 256 tecken.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="ordineradArtikelbenamning" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Artikelbenämning</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="ordineratProduktnamn" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Namn på produkten vid det tillfälle ordinationen upprättades</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="varunummer" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Artikelns varunummer</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="LakemedelsordinationRequest">
            <xs:sequence>                
                <xs:element name="behandlingsandamal" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Behandlingsändamål</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="dosering" type="OrdineradDoseringRequest" nillable="false" />
                            
                <xs:element name="foregaendeOriginalOrdinationsId" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Referens till föregående ordination i ordinationskedja</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="lakemedel" type="OrdineratLakemedelRequest" nillable="false" />
                            
                <xs:element name="notat" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Ordinationsnotat</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="utvarderingstidpunkt" type="xs:dateTime" minOccurs="0">
                    <xs:annotation>
