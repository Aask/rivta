<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="urn:riv:se.apotekensservice:nod:OrdineraInsattningResponder:2"
           targetNamespace="urn:riv:se.apotekensservice:nod:OrdineraInsattningResponder:2"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified"
           version="2.0">

    <!-- Globala element: Metod parametrar -->
    <xs:element name="OrdineraInsattning" type="OrdineraInsattningRequestType"/>
    <xs:element name="OrdineraInsattningResponse" type="OrdineraInsattningResponseType"/>

    <!-- Fel -->
    <xs:element name="ApplicationException" type="ApplicationException"/>
    <xs:element name="SystemException" type="SystemException"/>

            <xs:complexType name="OrdineraInsattningRequestType">
            <xs:sequence>
                <xs:element name="aktuellUnderlagstidpunkt" type="xs:dateTime" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Tidpunkt för senast uppläst underlag för patient. 
Anges för patienter med befintligt underlag.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="expeditionsunderlag" type="ForskrivningsinsattningRequest" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Underlag för förskrivningsinsättning.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="insattning" type="InsattningRequest" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Insättningsinformation.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="ordination" type="LakemedelsordinationRequest" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Ordinationsinformation skall journalföras.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="patient" type="PatientRequest" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Patientens identifikationsobjekt.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    
            <xs:complexType name="OrdineraInsattningResponseType">
            <xs:sequence>
                <xs:element name="originalordinationsId" type="xs:string" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Referens till ordination som satts in.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="uppdateradUnderlagstidpunkt" type="xs:dateTime" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Tidpunkt för uppdaterat underlag för patient.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            
                <xs:element name="vokresultat" type="VOKResultatResponse" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Resultat från VOK-INS.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>


<xs:complexType name="IntagstillfalleRequest">
            <xs:sequence>                
                <xs:element name="dagIPeriod" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Dag i perioden när intag skall göras.

Exempel: Intag ska ske varje måndag och onsdag och startdatum är på en måndag. Måndag Insättningsdatum = 2010-01-01, Periodlängd=7, Dag i period= 1. Onsdag Insättningsdatum = 2010-01-01, Periodlängd=7, Dag i period=3.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="intagsmangd" type="xs:double" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Intagsmängd per tillfälle. Null hanteras som enligt ordination.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="intagstillfalle" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Klockslag när patienten ska inta medicinering.</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="InsattningRequest">
            <xs:sequence>                
                <xs:element name="beslutstidpunkt" type="xs:dateTime" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Tidpunkt för uprättande av beslut. 
Kan vara insättning/utsättning/makulering beroende på typ av förändring.
Om ej angiven antas momentan tidpunkt.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="forandringstidpunkt" type="xs:dateTime" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Tidpunkt för ordinationsförändring. 
Kan vara insättningstidpunkt/utsättningstidpunkt/makuleringstidpunkt beroende på typ av förändring.
Om ej angiven antas momentan tidpunkt.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="insattningsorsak" type="xs:string" minOccurs="0" />
                            
                <xs:element name="ordinerandeVardpersonal" type="VardpersonalRequest" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Ordinerande vardpersonal.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="registrerandeVardpersonal" type="VardpersonalRequest" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Registrerande vårdpersonal</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="FodelsedatuminformationRequest">
            <xs:sequence>                
                <xs:element name="fodelsedatum" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Patientens födelsedatum.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="kon" type="xs:int" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Patientens kön.</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="VOKFelResponse">
            <xs:sequence>                
                <xs:element name="allvarlighetsgrad" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Kod för att beskriva hur allvarligt ett VOKfel är.

2 = Avvisad.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="felkod" type="xs:string" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Felkod enligt Vårdens ordinationskontroller (VOK).</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="felmeddelande" type="xs:string" nillable="false">
                    <xs:annotation>
