                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="TidsperiodRequest">
            <xs:sequence>                
                <xs:element name="tidsenhet" type="xs:string" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Tidsperiodens tidsenhet, anges som
VECKOR
DAGAR
TIMMAR
MINUTER</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="tidsintervall" type="IntervallRequest" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Tidsperiodens intervall</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="OrdineradDoseringRequest">
            <xs:sequence>                
                <xs:element name="doseringssteg" type="DoseringsstegRequest" nillable="false" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Doseringssteg.</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="DoseringsschemaRequest">
            <xs:sequence>                
                <xs:element name="intagstillfalle" type="IntagstillfalleRequest" nillable="false" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Beskriver tid och mängd för intag av läkemedel.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="periodlangd" type="xs:int" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Antal dagar som dosering skall gälla.
Vid regelbunden dosering anges periodlängd = 1
Vid oregelbunden dosering anges antal dagar som intervallet omfattar.
Exempelvis 2 om intag ska ske varannan dag.</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="FritextdoseringRequest">
            <xs:sequence>                
                <xs:element name="fritextdosering" type="xs:string" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Doseringstext</xs:documentation>
                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="KlockslagRequest">
            <xs:sequence>                
                <xs:element name="minut" type="xs:int" nillable="false">
                    <xs:annotation>
