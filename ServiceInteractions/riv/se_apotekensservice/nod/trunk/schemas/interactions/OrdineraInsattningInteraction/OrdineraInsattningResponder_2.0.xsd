                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="maxdosering" type="DoseringRequest" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Maxdosering.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="maxtid" type="xs:boolean" nillable="false">
                    <xs:annotation>
                        <xs:documentation>Maxtid</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="villkorsdosering" type="DoseringRequest" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Villkorsdosering.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                            
                <xs:element name="villkorstext" type="xs:string" minOccurs="0">
                    <xs:annotation>
