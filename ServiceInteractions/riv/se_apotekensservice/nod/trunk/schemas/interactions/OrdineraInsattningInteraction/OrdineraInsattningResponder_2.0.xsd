                    </xs:annotation>
                </xs:element>

			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
        </xs:complexType>

<xs:complexType name="DoseringsstegRequest">
            <xs:sequence>                
                <xs:element name="behandlingstid" type="TidsperiodRequest" minOccurs="0">
