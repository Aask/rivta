<wsdl:definitions name='SetSelectedCommissionForPersonInteraction'
	xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
	xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xs='http://www.w3.org/2001/XMLSchema'
	xmlns:tjsr='urn:riv:ehr:commission:SetSelectedCommissionForPersonResponder:1'
	xmlns:itr='urn:riv:itintegration:registry:1'
	xmlns:tns='urn:riv:ehr:commission:SetSelectedCommissionForPerson:1:rivtabp21'
	targetNamespace='urn:riv:ehr:commission:SetSelectedCommissionForPerson:1:rivtabp21'>

	<wsdl:documentation xml:lang='sv'>
		 Tjänsteinteraktionens namn: SetSelectedCommissionForPerson
		 Beskrivning: 
		   Tjänst för att välja medarbetaruppdrag till en person.
		   Personen anges antingen med HSA-id eller personnummer. Om man inte anger något av dessa eller att personnummer/HSA-id inte är giltigt returneras statuskod VALIDATIONERROR.
		   Om man inte anger något uppdrag, så har personen i fråga inte valt något uppdrag. 
		   Det betyder att när man sedan autentiserar sig, så får man ett SAML-intyg utan uppdrag.
		 Revisioner: 
		 Tjänstedomän: commission
		 Tjänsteinteraktionstyp: Fråga-Svar
		 WS-profil: RIVTABP21
		 Förvaltas av: Inera AB
	</wsdl:documentation>
	
	<wsdl:types>
		<xs:schema targetNamespace='urn:riv:ehr:commission:SetSelectedCommissionForPerson:1:rivtabp21'>
			<xs:import schemaLocation='SetSelectedCommissionForPersonResponder_1.0.xsd' namespace='urn:riv:ehr:commission:SetSelectedCommissionForPersonResponder:1' />
			<xs:import schemaLocation='../../core_components/itintegration_registry_1.0.xsd' namespace='urn:riv:itintegration:registry:1' />
		</xs:schema>
	</wsdl:types>
	
		
	<wsdl:message name='SetSelectedCommissionForPersonRequest'>
		<wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
			<wsdl:documentation xml:lang='sv'>Som logisk adress anges HSA-id för tjänsteproducenten.</wsdl:documentation>
		</wsdl:part>
	
		<wsdl:part name='parameters' element='tjsr:SetSelectedCommissionForPersonRequest' />
	</wsdl:message>

	<wsdl:message name='SetSelectedCommissionForPersonResponse'>
		<wsdl:part name='parameters' element='tjsr:SetSelectedCommissionForPersonResponse' />
	</wsdl:message>

	<wsdl:portType name='SetSelectedCommissionForPersonResponderInterface'>
		<wsdl:operation name='SetSelectedCommissionForPerson'>
			<wsdl:input message='tns:SetSelectedCommissionForPersonRequest' />
			<wsdl:output message='tns:SetSelectedCommissionForPersonResponse' />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name='SetSelectedCommissionForPersonResponderBinding' type='tns:SetSelectedCommissionForPersonResponderInterface'>
		<soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http' />
		<wsdl:operation name='SetSelectedCommissionForPerson'>
			<soap:operation soapAction='urn:riv:ehr:commission:SetSelectedCommissionForPersonResponder:1:SetSelectedCommissionForPerson' style='document' />
			<wsdl:input>
				<soap:header use='literal' message='tns:SetSelectedCommissionForPersonRequest' part='LogicalAddress' />
				<soap:body use='literal' parts='parameters' />
			</wsdl:input>
			<wsdl:output>
				<soap:body use='literal' />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name='SetSelectedCommissionForPersonResponderService'>
		<wsdl:port name='SetSelectedCommissionForPersonResponderPort' binding='tns:SetSelectedCommissionForPersonResponderBinding'>
			<soap:address location='http://tempuri.org' />
		</wsdl:port>
	</wsdl:service>
