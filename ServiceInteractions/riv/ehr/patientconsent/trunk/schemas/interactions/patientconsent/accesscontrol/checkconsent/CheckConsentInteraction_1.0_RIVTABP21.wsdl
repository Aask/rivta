<wsdl:definitions name='CheckConsentInteraction'
	xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
	xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xs='http://www.w3.org/2001/XMLSchema'
	xmlns:tjsr='urn:riv:ehr:patientconsent:accesscontrol:CheckConsentResponder:1'
	xmlns:itr='urn:riv:itintegration:registry:1'
	xmlns:tns='urn:riv:ehr:patientconsent:accesscontrol:CheckConsent:1:rivtabp21'
	targetNamespace='urn:riv:ehr:patientconsent:accesscontrol:CheckConsent:1:rivtabp21'>

	<wsdl:documentation>
		 Tjänsteinteraktionens namn: CheckConsent
		 Beskrivning: 
		   Tjänst som kontrollerar om det finns ett registrerat samtycke, alternativt intyg om nödsituation, gällande åtkomst för viss aktör (vårdenhet eller medarbetare).
		   Om ett giltigt intyg gällande åtkomst för angiven aktör hittas, kommer tjänsten att svara OK.
		 Revisioner: 
		 Tjänstedomän: patientconsent:accesscontrol
		 Tjänsteinteraktionstyp: Fråga-Svar
		 WS-profil: RIVTABP21
		 Förvaltas av: Inera AB
	</wsdl:documentation>
	
	<wsdl:types>
		<xs:schema targetNamespace='urn:riv:ehr:patientconsent:accesscontrol:CheckConsent:1:rivtabp21'>
			<xs:import schemaLocation='CheckConsentResponder_1.0.xsd' namespace='urn:riv:ehr:patientconsent:accesscontrol:CheckConsentResponder:1' />
			<xs:import schemaLocation='../../../../core_components/itintegration_registry_1.0.xsd' namespace='urn:riv:itintegration:registry:1' />
		</xs:schema>
	</wsdl:types>
	
	<wsdl:message name="UnauthorizedFault">
		<wsdl:documentation xml:lang="en">Undantag som kastas då behörighet saknas för att utföra begärd tjänst.
		</wsdl:documentation>
		<wsdl:part name="parameters" element="tjsr:UnauthorizedFault" />
	</wsdl:message>

	<wsdl:message name="ValidationErrorFault">
		<wsdl:documentation xml:lang="en">Undantag som kastas då en eller flera inparametrar innehåller felaktiga värden.
		</wsdl:documentation>
		<wsdl:part name="parameters" element="tjsr:ValidationErrorFault" />
	</wsdl:message>

	<wsdl:message name="InternalErrorFault">
		<wsdl:documentation xml:lang="en">Undantag som kastas då ett ohanterbart internt problem uppstått. Pågående tjänst kan ej utföras.
		</wsdl:documentation>
		<wsdl:part name="parameters" element="tjsr:InternalErrorFault" />
	</wsdl:message>

		
	<wsdl:message name='CheckConsentRequest'>
		<wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
			<wsdl:documentation>http://tempuri.org</wsdl:documentation>
		</wsdl:part>
	
		<wsdl:part name='parameters' element='tjsr:CheckConsentRequest' />
	</wsdl:message>

	<wsdl:message name='CheckConsentResponse'>
		<wsdl:part name='parameters' element='tjsr:CheckConsentResponse' />
	</wsdl:message>

	<wsdl:portType name='CheckConsentResponderInterface'>
		<wsdl:operation name='CheckConsent'>
			<wsdl:input message='tns:CheckConsentRequest' />
			<wsdl:output message='tns:CheckConsentResponse' />
			<wsdl:fault name="UnauthorizedFault" message="tns:UnauthorizedFault" />
			<wsdl:fault name="ValidationErrorFault" message="tns:ValidationErrorFault" />
			<wsdl:fault name="InternalErrorFault" message="tns:InternalErrorFault" />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name='CheckConsentResponderBinding' type='tns:CheckConsentResponderInterface'>
		<soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http' />
		<wsdl:operation name='CheckConsent'>
			<soap:operation soapAction='urn:riv:ehr:patientconsent:accesscontrol:CheckConsentResponder:1:CheckConsent' style='document' />
			<wsdl:input>
				<soap:header use='literal' message='tns:CheckConsentRequest' part='LogicalAddress' />
				<soap:body use='literal' parts='parameters' />
			</wsdl:input>
			<wsdl:output>
				<soap:body use='literal' />
			</wsdl:output>
			<wsdl:fault name="UnauthorizedFault">
				<soap:fault use="literal" name="UnauthorizedFault" />
			</wsdl:fault>
			<wsdl:fault name="ValidationErrorFault">
				<soap:fault use="literal" name="ValidationErrorFault" />
			</wsdl:fault>
			<wsdl:fault name="InternalErrorFault">
				<soap:fault use="literal" name="InternalErrorFault" />
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name='CheckConsentResponderService'>
		<wsdl:port name='CheckConsentResponderPort' binding='tns:CheckConsentResponderBinding'>
			<soap:address location='http://tempuri.org' />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
