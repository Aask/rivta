<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns:tns='urn:riv:clinicalprocess:activityprescription:prescribe:RegisterPrescriptionResponder:1'
           xmlns:core='urn:riv:clinicalprocess:activityprescription:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:prescribe:RegisterPrescriptionResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../core_components/clinicalprocess_activity_prescription_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:1' />

    <xs:element name='RegisterPrescription'
                type='tns:RegisterPrescriptionType' />

    <xs:element name='RegisterPrescriptionResponse'
                type='tns:RegisterPrescriptionResponseType' />

    <xs:complexType name='RegisterPrescriptionType'>
        <xs:sequence>

            <xs:element name="aktuellUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för senast uppläst underlag för patient.
                        Anges för patienter med befintligt underlag.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="expeditionsunderlag"
                        type="core:Forskrivningsinsattning"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Underlag för förskrivningsinsättning.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="insattning"
                        type="core:Insattning"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Insättningsinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordination"
                        type="core:Lakemedelsordination"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Ordinationsinformation skall journalföras.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patient"
                        type="core:Patient"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patientens identifikationsobjekt.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='RegisterPrescriptionResponseType'>
        <xs:sequence>
            <xs:element name="ordinationsId"
                        type="xs:string"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Referens till ordination som satts in.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="uppdateradUnderlagstidpunkt"
                        type="xs:dateTime"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för uppdaterat underlag för patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vokresultat"
                        type="core:VOKResultatResponse"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Resultat från VOK-INS.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='ResultCode'
                        type='core:ResultCodeEnum' />

            <xs:element name='comment'
                        type='xs:string'
                        minOccurs='0' />

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>
</xs:schema>
