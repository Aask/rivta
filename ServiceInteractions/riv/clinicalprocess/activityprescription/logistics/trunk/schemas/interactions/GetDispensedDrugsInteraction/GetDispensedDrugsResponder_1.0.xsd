<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns='urn:riv:clinicalprocess:activityprescription:logistics:GetDispensedDrugsResponder:1'
           xmlns:logistics='urn:riv:clinicalprocess:activityprescription:logistics:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:logistics:GetDispensedDrugsResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../../schemas/core_components/clinicalprocess_activityprescription_logistics_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:logistics:1' />

    <xs:element name='GetDispensedDrugs'
                type='GetDispensedDrugsType' />

    <xs:element name='GetDispensedDrugsResponse'
                type='GetDispensedDrugsResponseType'/>

    <xs:complexType name='GetDispensedDrugsType'>
        <xs:sequence>

            <xs:element name="patient"
                        type="logistics:Patient"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Patient vars läkemedelsförteckning skall hämtas.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="anropandeVardpersonal"
                        type="logistics:Vardpersonal"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Anropande vårdpersonal.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="forskrivarkod"
                        type="xs:string"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Förskrivarens individuella förskrivarkod.
                        Gruppförskrivarkoder får ej användas.
                        Valideras mot FORS. Obligatorisk om atkomsttyp = SAM
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="identifieradArbetsplats"
                        type="logistics:ArbetsplatsIdentifikation"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Identifierad arbetsplats.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="atkomsttyp"
                        type="logistics:atkomsttyp"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Treställig kod för typ av åtkomst.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='GetDispensedDrugsResponseType'>
        <xs:sequence>

            <xs:element name="lakemedelsforteckning"
                        type="logistics:Receptexpeditionsrad"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Patientens läkemedelsförteckning kompletterad med ordinationsmappningsinformation.
                        Om patienten saknar poster i läkemedelsförteckningen så returneras en tom lista.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patientinformation"
                        type="logistics:Patientinformation"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Information om patienten vars läkemedelsförteckning man hämtat.
                        Om patienten är markerad som 'Avliden' så returneras inga rader ur läkemedelsförteckningen.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="varningsnivaUppnadd"
                        type="xs:boolean"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Flagga som anger om förskrivarens antal läsningar det senaste dygnet
                        överskridit varningsnivå. Sant om varningsnivå har uppnåtts, annars
                        falskt.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <xs:element name='resultCode'
                        type='logistics:resultCodeEnum'
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Resultatkod för anropet.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='comment'
                        type='xs:string'
                        minOccurs='0'
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>
                        Kommentar till eventuella fel eller varningar.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

</xs:schema>
