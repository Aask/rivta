<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns='urn:riv:clinicalprocess:activityprescription:prescribe:ConfirmPrescriptionResponder:1'
           xmlns:tns='urn:riv:clinicalprocess:activityprescription:prescribe:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:prescribe:ConfirmPrescriptionResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../../schemas/core_components/clinicalprocess_activityprescription_prescribe_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:prescribe:1' />

    <xs:element name='ConfirmPrescription'
                type='ConfirmPrescriptionType' />

    <xs:element name='ConfirmPrescriptionResponse'
                type='ConfirmPrescriptionResponseType' />

    <xs:complexType name='ConfirmPrescriptionType'>
        <xs:sequence>
            <xs:element name="aktuellUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för senast uppläst underlag för patient.
                        Anges för patienter med befintligt underlag.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="insattning"
                        type="tns:Insattning"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Insättning av ej verifierad ordiantion.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="verifikation"
                        type="tns:Verifikation"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Verifiering av ej verifierad ordiantion.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordination"
                        type="tns:Lakemedelsordination"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Ej verifierad ordination som skall journalföras.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patient"
                        type="tns:Patient"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patientens identifikationsobjekt.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="forskrivningsId"
                        type="xs:long"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Förskrivningens id.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='ConfirmPrescriptionResponseType'>
        <xs:sequence>

            <xs:element name="ordinationsId"
                        type="xs:string"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Referens till ordination som verifierats.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="uppdateradUnderlagstidpunkt"
                        type="xs:dateTime"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för uppdaterat underlag för patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vokresultat"
                        type="tns:VOKResultat"
                        nillable="false"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Resultat från VOK-VER.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='resultCode'
                        type='tns:resultCodeEnum'>
                <xs:annotation>
                    <xs:documentation>Resultatkod för anropet.</xs:documentation>
                </xs:annotation>
            </xs:element>


            <xs:element name='comment'
                        type='xs:string'
                        minOccurs='0'>
                <xs:annotation>
                    <xs:documentation>Kommentar till eventuella fel eller varningar.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>
</xs:schema>