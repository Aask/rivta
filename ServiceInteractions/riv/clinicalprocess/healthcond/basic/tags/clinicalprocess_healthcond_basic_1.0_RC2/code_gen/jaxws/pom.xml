                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:basic:1=se.riv.clinicalprocess.healthcond.basic.v1</extraarg>
                  </extraargs>
                </wsdloption>
