                    <extraarg>urn:riv:clinicalprocess:healthcond:basic:DeleteMeasurementResponder:1=se.riv.clinicalprocess.healthcond.basic.deletemeasurementresponder.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:basic:DeleteMeasurementInitiator:1=se.riv.clinicalprocess.healthcond.basic.deletemeasurementinitiator.v1</extraarg>
