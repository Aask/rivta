			<soap:operation soapAction="urn:riv:ehr:ehrexchange:GetEhrExtractResponder:1:Ping"/>
			<wsdl:input>
				<soap:header use="literal" message="tns:PingRequest" part="LogicalAddress"/>
				<soap:body use="literal" parts="parameters"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
